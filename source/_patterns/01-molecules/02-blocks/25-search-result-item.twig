<div class="search-result-item">
	<a class="search-result-link" href="{{ link }}">{{ label }}</a>
	<p class="summary">{{ summary }}</p>
	{% if tags %}
		<ul class="tag-list">
			{% for item in tags %}
					<li><a href="{{ item.tag_link }}">{{ item.tag_label }}</a></li>
			{% endfor %}
		</ul>
	{% endif %}
</div>
